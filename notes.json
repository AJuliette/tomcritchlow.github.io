---
---
{% assign folder1 = site.pages | where_exp: "item" , "item.path contains 'notes'" | sort:"path" %}
{% assign notes = "" | split: ',' %}

{% for item in folder1 %}
{% if item.url contains "/wiki" %}
  {{item.url}} Contains wiki  
{% else %}
  {{item.url}}
  {% assign notes = notes | push: item %}
{% endif %}
{% endfor %}

[{% for item in notes %}{
  "url": "{{item.url}}",
  "content": {{ item.content | strip_html | strip_newlines | jsonify }}
}{% unless forloop.last %},{% endunless %}{% endfor %}]
