---
---
{% assign folder1 = site.pages | where_exp: "item" , "item.path contains 'notes'" | sort:"path" %}

[{% for item in folder1 %}
  {% if item.dir contains "/wiki/" %}
  {% else %}
  {
    "title": "{{item.url}}",
    "url": {{ item.url | prepend: site.baseurl | prepend: site.url | jsonify }},
    "content": {{ item.content | strip_html | strip_newlines | jsonify }}
  }{% unless forloop.last %},{% endunless %}{% endif %}{% endfor %}]